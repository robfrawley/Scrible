{
    "name"        : "scribe/jabiru",
    "description" : "A PHP 5.6 Markdown parser that supports a subset/superset of the original Markdown implementation.",
    "keywords"    : [ "markdown", "parser", "cli", "scribe", "jabiru", "ciconia" ],
    "type"        : "library",
    "homepage"    : "https://github.com/scribenet/jabiru",
    "license"     : "MIT",
    "authors"     : [
        {
            "name"  : "Rob Frawley 2nd",
            "email" : "rmf@scribe.tools"
        },
        {
            "name"  : "Kazuyuki Hayashi",
            "email" : "hayashi@valnur.net"
        }
    ],
    "require" : {
        "php"                      : ">=5.6.0",
        "symfony/options-resolver" : ">=2.5",
        "symfony/console"          : ">=2.5"
    },
    "require-dev" : {
        "symfony/finder"         : ">=2.5",
        "symfony/stopwatch"      : ">=2.5",
        "phpunit/phpunit"        : "~4.0",
        "scrutinizer/ocular"     : "~1.1"
    },
    "bin": [
        "bin/jabiru"
    ],
    "autoload": {
        "psr-4": {
            "Scribe\\": "src/"
        }
    }
}